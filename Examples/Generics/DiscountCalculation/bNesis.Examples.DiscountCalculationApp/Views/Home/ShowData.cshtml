
    <script>
        var data = @Html.Raw(@ViewBag.Responses);

        $( document ).ready(function() {
            var trs = '';

            data.reverse().forEach(function(row){
                var columns = '';
                Object.keys(row).forEach(function(col){
                    columns += '<td><div style="max-width: 450px; ' + ((col === 'Data' && row[col].length > 100) ? 'overflow:scroll;' : '') + '">' + row[col] + '</div></td>'; 
                });
                trs += '<tr>' + columns + '</tr>';
            });
            $('#table tbody').html(trs);
        });
    </script>
<div class="table-responsive mt-4 pt-4">
    <table id="table" class="table table-striped table-condensed">
        <thead>
            <tr>
                <th data-field="id">Item ID</th>
                <th data-field="provider">Provider</th>
                <th data-field="service">Service</th>
                <th data-field="data">Data</th>
                <th data-field="created">Created</th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
</div>
